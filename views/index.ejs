<html>
    <head>
        <title><%= title %></title>
    </head>
    <body>
        <%- include('navbar')%>
        <% messages.forEach(function(message) { %>
            <div class='card'>
                <h3><%= message.user %></h3>
                <p><%= message.text %></p>
                <a href="/<%= message.id %>">details</a>
            </div>
        <% }); %>
    </body>
</html>
